{
    "extends": ["prettier"],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "plugins": ["prettier"],
    "rules": {
      "prettier/prettier": "error",
      "spaced-comment": ["error", "always", {
        "exceptions": ["?", "!"]
      }]
    },
    "parserOptions": {
      "ecmaVersion": 2021,
      "sourceType": "module"
    },
  }
  